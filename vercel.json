{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/crons/sync_mqtt.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "dest": "/api/index.py"
    }
  ],
  "crons": [
    {
      "path": "/api/crons/sync_mqtt",
      "schedule": "* * * * *"
    }
  ]
}